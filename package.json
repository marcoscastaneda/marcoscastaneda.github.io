{
  "name": "preact-rename-me",
  "version": "1.2.0",
  "main": "index.js",
  "author": "Marcos J. Castaneda",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV='production' webpack",
    "dev": "webpack-dev-server --progress --inline --hot --colors",
    "_test": "ava --verbose --tap | tap-spec"
  },
  "dependencies": {
    "preact": "^7.1.0"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/*.{js,jsx}"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-loader": "^1.6.1",
    "preact": "^7.1.0",
    "webpack": "^1.14.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^1.16.2"
  }
}
